# required varibales: iam_endpoint, token, description, user_id

curl -i -k -H "X-Auth-Token:$token" -H "Content-Type:application/json;charset=utf8" -X POST https://$iam_endpoint/v3.0/OS-CREDENTIAL/credentials -d '{"credential":{"description":"IAMDescription","user_id":"aa31c6f5a9b246d7a459f37172e3030a"}}'
